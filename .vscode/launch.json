
// Esta configuración permite abrir automáticamente en Edge cualquier 
// archivo HTML que forme parte de este Workspace.

{
  "version": "0.2.0",
  "configurations": [
    
    {
      "type": "msedge",
      "request": "launch",
      "name": "Abrir HTML en Edge",
      "url": "file://${file}",
      "webRoot": "${workspaceFolder}"
    }
  ]
}