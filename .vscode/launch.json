
// Esta configuración permite abrir automáticamente en Chrome cualquier 
// archivo HTML que forme parte de este Workspace.

{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "chrome",
      "request": "launch",
      "name": "OpenHTML",
      "file": "${file}",
      "webRoot": "${workspaceFolder}"
    }
  ]
}
